<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
"-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- 制定mysql的驱动包的路径 千万别放中文路径下 -->
	<classPathEntry location="D:\myRepository\repository\mysql\mysql-connector-java\5.1.47\mysql-connector-java-5.1.47.jar" />
	<!-- 配置数据源和生成的代码所存放的位置 -->
	<context id="context1">
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://192.168.126.128:3306/school" 
			userId="root"
			password="123456" />
		<!-- 所生成的实体类的位置默认资源包src -->
		<javaModelGenerator targetPackage="com.rt.ecps.model" targetProject="ecps-core" />
		<!-- 所生成的sqlMap的影射文件的位置,默认资源包src -->
		<sqlMapGenerator targetPackage="com.rt.ecps.mapper" targetProject="ecps-core" />
		
		<!-- 为哪些表生成代码 tableName:表名 schema:不用填写 -->
		
		<table schema="" tableName="ROLE" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
		</table>
	</context>
</generatorConfiguration>
